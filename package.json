{
  "name": "usernolan",
  "license": "CC0-1.0",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:markup": "nodemon -x ts-node-esm -i src/main.ts -i src/images.ts src/gen.ts",
    "build:markup": "ts-node-esm src/gen.ts",
    "release:markup": "ts-node-esm src/gen.ts --release",
    "build:css": "postcss src/*.css --use autoprefixer --replace --no-map",
    "build:images": "ts-node-esm src/images.ts",
    "build": "tsc && yarn build:css && yarn release:markup && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@thi.ng/hiccup": "^4.2.29",
    "@thi.ng/transducers": "^8.3.30",
    "@types/js-beautify": "^1.13.3",
    "@types/node": "^18.11.18",
    "@types/sharp": "^0.31.1",
    "autoprefixer": "^10.4.13",
    "js-beautify": "^1.14.7",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "sharp": "^0.31.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^4.0.4"
  },
  "dependencies": {
    "shufflejs": "^6.1.0"
  }
}
